{
  "name": "@jiangzhongxi0322/tflow",
  "version": "0.0.6",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist es lib",
    "start": "pnpm build:watch",
    "build:esm": "tsc --module es6 --target es6 --outDir ./es",
    "build:cjs": "tsc --module commonjs --target es5 --outDir ./lib",
    "build:umd": "rollup -c",
    "build:watch": "pnpm build:esm --w",
    "build:watch:esm": "pnpm build:esm --w",
    "build:watch:cjs": "pnpm build:cjs --w",
    "build:ts": "run-p build:cjs build:esm build:umd",
    "build:less": "node ./scripts/less.js",
    "build": "run-s build:ts build:less",
    "prebuild": "run-s lint clean",
    "prepare": "pnpm build",
    "release": "pnpm publish  --access=public --force",
    "lint": "run-s lint:ts  ",
    "lint:ts": "eslint 'src/**/*.{js,ts}?(x)' --fix",
    "lint:style": "stylelint 'src/**/*.less' --syntax less --fix",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "jest --verbose true",
    "test:coverage": "jest --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@antv/layout": "^0.1.22",
    "@antv/x6": "^1.30.1",
    "@antv/x6-react-components": "^1.1.15",
    "@antv/x6-react-shape": "^1.5.2",
    "@jiangzhongxi0322/tflow-core": "workspace:*",
    "@jiangzhongxi0322/tflow-extension": "workspace:*",
    "@jiangzhongxi0322/tflow-hook": "workspace:*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "fs-extra": "^10.0.0",
    "less": "^4.1.2",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^11.2.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.6.0",
    "antd": "5.18.0",
    "lodash": "^4.17.20",
    "react": "^16.8.0  || ^17.0.0",
    "react-dom": "^16.8.0  || ^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/antvis/XFlow.git"
  },
  "description": "## Getting Started",
  "bugs": {
    "url": "https://github.com/antvis/XFlow/issues"
  },
  "homepage": "https://github.com/antvis/XFlow#readme",
  "directories": {
    "lib": "lib"
  },
  "author": "jiang"
}
